<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CapstoneProject"
        tools:targetApi="31">
        <activity
            android:name=".View.BottomNavigation"
            android:exported="false"
            android:label="@string/title_activity_bottom_navigation" />
        <activity
            android:name=".View.ButtomNavigation"
            android:exported="false"
            android:label="@string/title_activity_buttom_navigation" />
        <activity
            android:name=".View.ButtomNavigationView"
            android:exported="false"
            android:label="@string/title_activity_buttom_navigation_view" />
        <activity
            android:name=".View.Register.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".View.Login.LoginActivity"
            android:exported="true" />
        <activity
            android:name=".View.Main.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>